{
    "version": 2,
    "builds": [
        {
            "src": "/src/server.ts",
            "use": "@vercel/node@1.9.1",
            "config": {
                "includeFiles": "/src/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ]
}